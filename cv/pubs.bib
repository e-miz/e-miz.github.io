@inproceedings{goryachevAcousticTestsLorentz2016,
  title       = {Acoustic Tests of {{Lorentz}} Symmetry Using {{Bulk Acoustic Wave}} Quartz Oscillators},
  author      = {Goryachev, M. and Lo, A. and Haslinger, Ph and Mizrachi, E. and Anderegg, L. and Müller, H. and Hohensee, M. and Tobar, M. E.},
  date        = {2016-08-07},
  eprint      = {1607.07179},
  eprinttype  = {arxiv},
  eprintclass = {hep-ph, physics:physics},
  publisher   = {{arXiv}},
  location    = {{Bloomington, Indiana}},
  doi         = {10.48550/arXiv.1607.07179},
  url         = {http://arxiv.org/abs/1607.07179},
  urldate     = {2023-01-02},
  abstract    = {A new method of probing Lorentz invariance in the neutron sector is described. The method is baed on stable quartz bulk acoustic wave oscillators compared on a rotating table. Due to Lorentz-invariance violation, the resonance frequencies of acoustic wave resonators depend on the direction in space via a corresponding dependence of masses of the constituent elements of solids. This dependence is measured via observation of oscillator phase noise built around such devices. The first such experiment now shows sensitivity to violation down to the limit \$\textbackslash tilde\{c\}\^n\_Q=(-1.8\textbackslash pm2.2)\textbackslash times 10\^\{-14\}\$ GeV. Methods to improve the sensitivity are described together with some other applications of the technology in tests of fundamental physics.},
  eventtitle  = {Seventh {{Meeting}} on {{CPT}} and {{Lorentz Symmetry}}},
  keywords    = {mypublication},
  file        = {J:\My Drive\Sources\Zotero\Goryachev et al. - 2016 - Acoustic tests of Lorentz symmetry using Bulk Acou.pdf}
}

@inproceedings{kamdinAbsoluteElectronExtraction2016,
  title      = {Absolute {{Electron Extraction Efficiency}} of {{Liquid Xenon}}},
  booktitle  = {Bulletin of the {{American Physical Society}}},
  author     = {Kamdin, K. and Mizrachi, E. and Morad, J. A. and Sorensen, P.},
  date       = {2016-04-16},
  volume     = {Volume 61, Number 6},
  publisher  = {{American Physical Society}},
  location   = {{Salt Lake City, Utah}},
  url        = {http://meetings.aps.org/link/BAPS.2016.APR.B16.7},
  urldate    = {2023-01-02},
  abstract   = {Dual phase liquid/gas xenon time projection chambers (TPCs) currently set the world’s most sensitive limits on weakly interacting massive particles (WIMPs), a favored dark matter candidate. These detectors rely on extracting electrons from liquid xenon into gaseous xenon, where they produce proportional scintillation. The proportional scintillation from the extracted electrons serves to internally amplify the WIMP signal; even a single extracted electron is detectable. Credible dark matter searches can proceed with electron extraction efficiency (EEE) lower than 100\textbackslash\%. However, electrons systematically left at the liquid/gas boundary are a concern. Possible effects include spontaneous single or multi-electron proportional scintillation signals in the gas, or charging of the liquid/gas interface or detector materials. Understanding EEE is consequently a serious concern for this class of rare event search detectors. Previous EEE measurements have mostly been relative, not absolute, assuming efficiency plateaus at 100\textbackslash\%. I will present an absolute EEE measurement with a small liquid/gas xenon TPC test bed located at Lawrence Berkeley National Laboratory.},
  eventtitle = {{{APS April Meeting}} 2016},
  keywords   = {mypublication},
  annotation = {Presentation}
}

@article{loAcousticTestsLorentz2016,
  title        = {Acoustic Tests of {{Lorentz}} Symmetry Using Quartz Oscillators},
  author       = {Lo, Anthony and Haslinger, Philipp and Mizrachi, Eli and Anderegg, Loic and Müller, Holger and Hohensee, Michael and Goryachev, Maxim and Tobar, Michael E.},
  date         = {2016-02-24},
  journaltitle = {Physical Review X},
  shortjournal = {Phys. Rev. X},
  volume       = {6},
  number       = {1},
  eprint       = {1412.2142},
  eprinttype   = {arxiv},
  eprintclass  = {gr-qc, physics:hep-th},
  pages        = {011018},
  issn         = {2160-3308},
  doi          = {10.1103/PhysRevX.6.011018},
  url          = {http://arxiv.org/abs/1412.2142},
  urldate      = {2023-01-02},
  abstract     = {We propose and demonstrate a test of Lorentz symmetry based on new, compact, and reliable quartz oscillator technology. Violations of Lorentz invariance in the matter and photon-sector of the standard model extension (SME) generate anisotropies in particles' inertial masses and the elastic constants, giving rise to measurable anisotopies in the resonance frequencies of acoustic modes in solids. A first realization of such a "phonon-sector" test of Lorentz symmetry using room-temperature SC-cut crystals provides a limit of \$\textbackslash tilde c\_Q\^\{\textbackslash rm n\}=(-1.8 \textbackslash pm 2.2)\textbackslash times 10\^\{-14\}\$\textbackslash,GeV on the most weakly constrained neutron-sector \$c-\$coefficient of the SME. Future experiments with cryogenic oscillators promise significant improvements in accuracy, opening up the potential for improved limits on Lorentz violation in the neutron, proton, electron and photon sector.},
  keywords     = {mypublication,selectedworks},
  file         = {J:\My Drive\Sources\Zotero\Lo et al. - 2016 - Acoustic tests of Lorentz symmetry using quartz os.pdf}
}

