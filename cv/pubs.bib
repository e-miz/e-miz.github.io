@article{bernardThermodynamicStabilityXenondoped2023,
  title        = {Thermodynamic Stability of Xenon-Doped Liquid Argon Detectors},
  author       = {Bernard, E. P. and Mizrachi, E. and Kingston, J. and Xu, J. and Pereverzev, S. V. and Pershing, T. and Smith, R. and Prior, C. G. and Bowden, N. S. and Bernstein, A. and Hall, C. R. and Pantic, E. and Tripathi, M. and McKinsey, D. N. and Barbeau, P. S.},
  date         = {2023-10-20},
  journaltitle = {Physical Review C},
  shortjournal = {Phys. Rev. C},
  volume       = {108},
  number       = {4},
  pages        = {045503},
  publisher    = {{American Physical Society}},
  doi          = {10.1103/PhysRevC.108.045503},
  url          = {https://link.aps.org/doi/10.1103/PhysRevC.108.045503},
  urldate      = {2023-10-29},
  abstract     = {Liquid argon detectors are employed in a wide variety of nuclear and particle physics experiments. The addition of small quantities of xenon to argon modifies its scintillation, ionization, and electroluminescence properties and can improve its performance as a detection medium. However, a liquid argon-xenon mixture can develop instabilities, especially in systems that require phase transitions or that utilize high xenon concentrations. In this work, we analyze the causes of these instabilities and describe a small (liter-scale) apparatus with a unique cryogenic circuit specifically designed to handle argon-xenon mixtures. The system is capable of condensing argon gas mixed with O(1\%) xenon by volume and maintains a stable liquid mixture near the xenon saturation limit while actively circulating it in the gas phase. We also demonstrate control over instabilities that develop when the detector condition is allowed to deviate from optimized settings. This progress enables future liquid argon detectors to benefit from the effects of high concentrations of xenon doping, such as more efficient detection of low-energy ionization signals. This work also develops tools to study and mitigate instabilities in large argon detectors that use low concentration xenon doping.},
  keywords     = {mypublication,selectedworks},
  file         = {G\:\\My Drive\\Sources\\Zotero\\Bernard et al. - 2023 - Thermodynamic stability of xenon-doped liquid argo.pdf;C\:\\Users\\emiz\\Zotero\\storage\\KPAURRK9\\PhysRevC.108.html}
}

