@online{xuSearchMigdalEffect2023,
  title       = {Search for the {{Migdal}} Effect in Liquid Xenon with {{keV-level}} Nuclear Recoils},
  author      = {Xu, Jingke and Adams, Duncan and Lenardo, Brian and Pershing, Teal and Mannino, Rachel and Bernard, Ethan and Kingston, James and Mizrachi, Eli and Lin, Junsong and Essig, Rouven and Mozin, Vladimir and Kerr, Phil and Bernstein, Adam and Tripathi, Mani},
  date        = {2023-07-24},
  eprint      = {2307.12952},
  eprinttype  = {arxiv},
  eprintclass = {hep-ex, physics:physics},
  doi         = {10.48550/arXiv.2307.12952},
  url         = {http://arxiv.org/abs/2307.12952},
  urldate     = {2023-10-29},
  abstract    = {The Migdal effect predicts that a nuclear recoil interaction can be accompanied by atomic ionization, allowing many dark matter direct detection experiments to gain sensitivity to sub-GeV masses. We report the first direct search for the Migdal effect for M- and L-shell electrons in liquid xenon using 7.0\$\textbackslash pm\$1.6 keV nuclear recoils produced by tagged neutron scatters. Despite an observed background rate lower than that of expected signals in the region of interest, we do not observe a signal consistent with predictions. We discuss possible explanations, including inaccurate predictions for either the Migdal rate or the signal response in liquid xenon. We comment on the implications for direct dark-matter searches and future Migdal characterization efforts.},
