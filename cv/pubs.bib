@unpublished{mizrachiAbsoluteMeasurementElectron2016,
  type       = {Poster},
  title      = {Absolute {{Measurement}} of the {{Electron Extraction Efficiency}} of {{Liquid Xenon}}},
  author     = {Mizrachi, E. and Kamdin, K. and Morad, J. A. and Sorensen, P.},
  date       = {2016-03-12},
  abstract   = {Weakly interacting massive particles (WIMPs) are promising candidates in the search for dark matter. WIMP interactions in a dual phase liquid xenon time projection chamber (TPC) are theorized to produce photons and ionized electrons, the latter of which are extracted out of the liquid into a gaseous region of xenon. Proportional scintillation from the electrons occurs as they pass the liquid-gas interface, and this additional light is used to verify that a WIMP- xenon interaction has taken place. The electron extraction efficiency (EEE) is crucial for ensuring that an accurate signal is received. Consistently inefficient extraction results in leftover charges in the detector, which may scintillate or accumulate on detector materials. This behavior is detrimental to experiments which demand record breaking levels of sensitivity. Moreover, previous liquid xenon EEE measurements have mostly been relative. Using a, dual phase liquid xenon TPC at Lawrence Berkeley National Laboratory, we aim to provide an absolute EEE measurement instead.},
  eventtitle = {{{UC LEADS Student Research}} Symposium},
  venue      = {{Davis, California}},
  keywords   = {mypresentation,selectedworks}
}

